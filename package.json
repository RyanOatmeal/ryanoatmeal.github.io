  "description": "My doodles",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanOatmeal/ryanoatmeal.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/RyanOatmeal/ryanoatmeal.github.io/issues"
  },
  "homepage": "https://github.com/RyanOatmeal/ryanoatmeal.github.io#readme",
  "scripts": {
    "compile": "npx tsc && cpx \"src/**/*.{js,wasm,d.ts,json}\" docs/scripts",
    "start": "npx http-server docs",
    "prepare": "husky",
    "format": "npx prettier ./src --write",
    "format-check": "npx prettier ./src --check",
    "lint": "npx eslint ./src",
    "test": "jest",
    "pre-commit": "npm run compile && npm run format-check && npm run lint && npm run test"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/jest": "^30.0.0",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jiti": "^2.6.1",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "cpx": "^1.5.0"
  },
  "type": "module"
}
